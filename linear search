#include <iostream>

using namespace std;
int linearbinary(int arr[],int n ,int key){
    for(int i=0; i<n; i++){
       if(arr[i]==key){
           return i;
       }
    }
       return -1;
}
int main(){
    int n;
    int arr[n];
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int key;
    cin>>key;
    cout<<linearbinary(arr,n,key)<<endl;
    
    return 0;
}
